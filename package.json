{
  "name": "react-boilerplate",
  "version": "0.0.1",
  "description": "boilerplate for react",
  "scripts": {
    "server": "node server",
    "start": "yarn dev",
    "dev": "./node_modules/.bin/webpack-dev-server --mode development --config webpack/dev.config.js",
    "build": "./node_modules/.bin/webpack --mode production --config webpack/prod.config.js",
    "jslint": "./node_modules/.bin/eslint src --ext .jsx,.js -f table",
    "csslint": "./node_modules/.bin/stylelint src/\"**/*.scss\"",
    "lint": "yarn jslint && yarn csslint",
    "test": "./node_modules/.bin/jest -c tests/test.config.json",
    "precommit": "yarn lint && yarn test",
    "clean": "rm -rf build/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexeyvax/react-boilerplate.git"
  },
  "author": "alexeyvax",
  "license": "ISC",
  "keywords": [
    "react",
    "redux",
    "boilerplate",
    "react-redux-boilerplate",
    "react-redux-template",
    "starter"
  ],
  "bugs": {
    "url": "https://github.com/alexeyvax/react-boilerplate/issues"
  },
  "homepage": "https://github.com/alexeyvax/react-boilerplate#readme",
  "dependencies": {
    "autoprefixer": "^8.2.0",
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.18.2",
    "classnames": "^2.2.5",
    "express": "^4.16.3",
    "lodash": "^4.17.5",
    "normalize.css": "^8.0.0",
    "prop-types": "^15.6.1",
    "react": "^16.3.0",
    "react-css-themr": "^2.1.2",
    "react-dom": "^16.3.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.16.0",
    "reselect": "^3.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-jsx-control-statements": "^2.2.0",
    "eslint-plugin-react": "^7.7.0",
    "fs": "^0.0.1-security",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.4.3",
    "jsx-control-statements": "^3.2.8",
    "node-sass": "^4.8.3",
    "postcss-loader": "^2.1.3",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.3.0",
    "redux-mock-store": "^1.5.1",
    "sass-loader": "^6.0.7",
    "sinon": "^4.5.0",
    "style-loader": "^0.20.3",
    "stylelint": "^9.2.0",
    "stylelint-config-standard": "^18.2.0",
    "webpack": "^4.4.1",
    "webpack-cli": "^2.0.13",
    "webpack-dev-server": "^3.1.1"
  },
  "husky": {
    "hooks": {
      "precommit": "yarn lint && yarn test"
    }
  },
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=3.0.0",
    "yarn": ">=1.1.0"
  }
}
